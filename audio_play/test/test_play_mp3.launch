<launch>
  <arg name="rosbag_option" default="" />
  <arg name="bagfile_names" default="$(find audio_play)/sample/mp3.bag" />

  <include file="$(find audio_play)/launch/play.launch">
    <arg name="dst" value="/tmp/tmp.mp3" />
  </include>
  <node name="rosbag_player" pkg="rosbag" type="play"
        args="$(arg rosbag_option) $(arg bagfile_names)"/>
</launch>
